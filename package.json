{
  "name": "vernissage",
  "version": "0.1.0",
  "description": "Photography-focused portfolio theme for Eleventy based on Tailwind",
  "scripts": {
    "build": "pnpm run cleanup && tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.js",
    "dev": "tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.js --serve --port 3000 --incremental",
    "cleanup": "pnpm dlx rimraf _site _tmp .cache",
    "serve-build": "pnpm dlx http-server _site --port 4000"
  },
  "keywords": [],
  "author": "Niklas Graeber <dev@n1klas.net>",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-vite": "^7.0.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "alpinejs": "^3.14.9",
    "autoprefixer": "^10.4.21",
    "jsx-async-runtime": "^1.0.3",
    "photoswipe": "^5.4.4",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.4"
  }
}
