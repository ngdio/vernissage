{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "jsx-async-runtime",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "target": "ES2020",
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "_site"]
}
